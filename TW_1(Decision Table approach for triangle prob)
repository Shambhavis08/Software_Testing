//program 1(Decision Table approach for triangle prob)
#include<stdio.h>
int main(){
 int a,b,c;
 char istriangle;
 printf("Enter a, b, c: ");
 scanf("%d %d %d",&a,&b,&c);

 if(a+b>c && b+c>a && a+c>b) istriangle='y';
 else istriangle='n';

    if(istriangle=='y'){
 if(a==b && b==c)printf("Equilateral Triangle");
 else if((a!=b) && (b!=c) && (c!=a)) printf("Scalene Triangle");
 else printf("Isosceles Triangle");
 }else{
 printf("Not a Triangle\n");
 }
 return 0;
}
